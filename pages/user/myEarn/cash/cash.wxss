/* pages/user/myEarn/cash/cash.wxss */
.cashInput{width: 100%;text-align: center;line-height: 68rpx;color: #fff;font-size: 32rpx;margin-top: 160rpx;display: flex;flex-direction: row;justify-content: center;}
.cashInput input{width: 200rpx;height: 68rpx;border: 2rpx solid #999;border-radius: 10rpx;vertical-align: middle;}
.cashInput .balance{font-size: 28rpx;color: #999;margin: 30rpx auto 0;line-height: 28rpx;}
.cashInput .last{margin-left: 30rpx;}
.warning{margin: 30rpx auto;font-size: 24rpx;color: #999;text-align: center;line-height: 24rpx;}
.withdrawCash{width: 260rpx;height: 80rpx;border-radius: 10rpx;background: #8050eb;line-height: 80rpx;font-size: 32rpx;text-align: center;margin: 50rpx auto;color: #fff;}
.hoverViewThis{position: absolute;z-index: 3;top: 0;bottom: 0;left: 0;right: 0;}
.warningView{width: 460rpx;height: 208rpx;background: rgba(255, 255, 255, .96);border-radius: 20rpx;margin: 320rpx auto;display: flex;flex-direction: column;justify-content: center;}
.warningView image{margin: 30rpx auto 0;}
.warningView .warningWord{font-size: 32rpx;color: #333;line-height: 40rpx;text-align: center;margin: 18rpx 10rpx;}