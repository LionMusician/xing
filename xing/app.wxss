/**app.wxss**/
page{background: #1A1D26;height: 100%;color: #fff;}
view{box-sizing: border-box;}
.cells{width: 730rpx;margin: 19rpx auto;color: #fff;font-size: 32rpx;}
.cell{background: #21212b;padding: 20rpx;height: 72rpx;line-height: 72rpx;width: 690rpx;margin: 0rpx auto 1rpx;box-sizing: content-box;}
.cell>view{display: inline-block;}
.cell_ft{float: right;}
.borderTop{border-radius: 10rpx 10rpx 0 0;}
.borderBottom{border-radius: 0 0 10rpx 10rpx;}
.borderAll{border-radius: 10rpx;}
.nextBtn{width: 18rpx;height: 34rpx;vertical-align: middle;}

.goods{width: 730rpx;margin: 0 auto 12rpx;background: #21212b;border-radius: 20rpx;height: 180rpx;}
.goods image{width: 140rpx;height: 140rpx;margin: 20rpx;vertical-align: top;float: left;}
.goods .title{font-size: 30rpx;color: #fff;width: 500rpx;margin-top: 34rpx;float: left;}