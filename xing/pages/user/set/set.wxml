<!--pages/user/set/set.wxml-->
<view class='container'>
  <view class='cells'>
    <view class='cell borderTop' bindtap='toUserInfo'>
      <view class='cell_hd'>个人资料</view>
      <view class='cell_ft'><image src='../../images/user/next@2x.png' class='nextBtn'></image></view>
    </view>
    <view class='cell borderBottom' bindtap='toSecret'>
      <view class='cell_hd'>隐私设置</view>
      <view class='cell_ft'><image src='../../images/user/next@2x.png' class='nextBtn'></image></view>
    </view>
  </view>
  <view class='cells'>
    <view class='cell borderAll' bindtap='toAddress '>
      <view class='cell_hd'>收货地址</view>
      <view class='cell_ft'><image src='../../images/user/next@2x.png' class='nextBtn'></image></view>
    </view>
  </view>
</view>
